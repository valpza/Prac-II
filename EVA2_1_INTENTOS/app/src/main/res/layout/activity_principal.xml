<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:layout_height="match_parent"
    tools:context="com.example.iowner.eva2_1_intentos.Principal">

    <TextView
        android:id="@+id/textView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Numero Telefonico"
        android:textColor="@android:color/background_dark" />

    <EditText
        android:id="@+id/editTxtNumero"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="number" />

    <Button
    android:id="@+id/btnLlamar"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:onClick="click"
    android:text="Llamar" />
    <Button
    android:id="@+id/btnLlamar1"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:onClick="clickLlamar"
    android:text="Llamada directa" />
    <Button
        android:id="@+id/btnLlamar2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="clickBuscar"
        android:text="Llamada directa" />
</LinearLayout>
